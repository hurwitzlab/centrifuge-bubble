{
  "name":"centrifuge-bubble-test",
  "appId": "centrifuge-bubble-0.0.6",
  "batchQueue": "normal",
  "maxRunTime": "02:00:00",
  "memoryPerNode": "32GB",
  "nodeCount": 1,
  "processorsPerNode": 16,
  "archive": true,
  "archiveSystem": "data.iplantcollaborative.org",
  "inputs": {
    "QUERY": [ "kyclark/data/dolphin/reports" ]
  },
  "parameters": {
    "TITLE": "Dolphin Blowhole",
    "EXCLUDE_SPECIES": "synthetic construct, Homo sapiens",
    "MIN_PROPORTION": "0.02"
  },
  "notifications": [
    {
      "url":"https://requestbin.agaveapi.co/?job_id=${JOB_ID}&status=${JOB_STATUS}",
      "event":"*",
      "persistent":true
    },
    {
      "url":"kyclark@gmail.com",
      "event":"FINISHED",
          "persistent":false
    },
    {
      "url":"kyclark@gmail.com",
      "event":"FAILED",
      "persistent":false
    }
  ]
}
